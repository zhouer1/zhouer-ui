{
  "name": "zhouer-ui",
  "type": "module",
  "version": "1.0.0",
  "description": "personal vue3 components lib for my study",
  "author": "zhouer1 <zhouer1mail@qq.com>",
  "license": "MIT",
  "keywords": ["vue3", "vue", "components"],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./lib/*": "./lib/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "files": ["dist", "es", "lib"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:style": "vite build --config vite.style.config.ts"
  },
  "dependencies": {
    "@zhouer-ui/utils": "workspace:*"
  }

}
